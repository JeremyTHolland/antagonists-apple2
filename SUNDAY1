10  REM ** CREATE DATAFILE **
20  START = 10000
30  LSTADD = 21631
40  HIMEM: START
50  HOME
60  D$ = CHR$ (4).R$ = CHR$ (13)
70  PRINT "SAVING..."
80  PRINT D$;"OPEN DATAFILE"
90  PRINT D$;"WRITE DATAFILE"
100 FOR I = START TO LSTADD
110 CH = PEEK (I)
120 IF CH = 0 THEN NEXT
130 PRINT CHR$ (CH);
140 NEXT
150 PRINT R$;D$;"CLOSE DATAFILE"
160 END